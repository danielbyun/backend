<% include partials/header %>
    <h1> what up yall these are the posts </h1>

    <h3> for loop</h3>

    <% for(let i = 0; i < posts.length; i++){%>
    <li><%= posts[i].title %> - written by <strong><%= posts[i].author %></strong></li>
    <% } %>

    <h3> forEach loop </h3>

    <% posts.forEach(function(posts){%>
        <li><%= posts.title %> - written by <strong><%= posts.author %></strong></li>
    <% }) %>
<% include partials/footer %>